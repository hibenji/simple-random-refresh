{
  "manifest_version": 3,
  "name": "Benji Reload Random",
  "version": "1.2",
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "refresh.png"
    }
  },
  "icons": {
    "16": "refresh.png"
  },
  "permissions": ["backgroud", "storage", "activeTab", "scripting", "tabs", "unlimitedStorage"]

}
