{
  "manifest_version": 3,
  "name": "Benji Reload Random - Alarm Update",
  "version": "1.6",
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "refresh.png"
    }
  },
  "icons": {
    "16": "refresh.png"
  },
  "host_permissions": [
    "*://*/*"
  ],
  "permissions": ["alarms", "storage", "activeTab", "scripting", "tabs", "unlimitedStorage", "cookies"]

}
